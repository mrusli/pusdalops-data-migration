<?page title="KejadianPelakuListDialog" contentType="text/html;charset=UTF-8"?>
<zk>
<window width="400px" height="520px" border="normal"
	id="kejadianPelakuListDialogWin" apply="${kejadianPelakuListDialogControl}">
	<vlayout>
		<label id="kejadianPelakuStatementLabel"/>
	</vlayout>
	<listbox id="kejadianPelakuListbox" height="280px">
		<listhead>
			<listheader label="Pelaku Kejadian" width="285px"></listheader>
		</listhead>
	</listbox>
	<hlayout style="padding-top: 5px;">
		<button id="selectButton" label="Pilih" sclass="comButton" disabled="true"></button>
		<button id="addButton" label="Tambah" sclass="comButton"></button>
		<button id="cancelButton" label="Cancel" sclass="comButton"></button>	
	</hlayout>
</window>
</zk>