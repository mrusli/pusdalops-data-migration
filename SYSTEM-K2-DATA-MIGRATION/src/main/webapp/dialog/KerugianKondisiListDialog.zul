<?page title="KerugianKondisiListDialog" contentType="text/html;charset=UTF-8"?>
<zk>
<window width="400px" height="520px" border="normal"
	id="kerugianKondisiListDialogWin" apply="${kerugianKondisiListDialogControl}">
	<vlayout>
		<label id="kerugianKondisiStatementLabel"/>
	</vlayout>
	<listbox id="kerugianKondisiListbox" height="280px">
		<listhead>
			<listheader label="Kondisi Kerugian" width="285px"></listheader>
		</listhead>
	</listbox>
	<hlayout style="padding-top: 5px;">
		<button id="selectButton" label="Pilih" sclass="comButton" disabled="true"></button>
		<button id="addButton" label="Tambah" sclass="comButton"></button>
		<button id="cancelButton" label="Cancel" sclass="comButton"></button>	
	</hlayout>
</window>
</zk>